</head>
<body class="cbp-spmenu-push">
    <div class="main-content">
        <!--left-fixed -navigation-->
        <div class=" sidebar" role="navigation">
            <div class="navbar-collapse">
                <nav class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left" id="cbp-spmenu-s1">
                    @if (CurrentSession.User != null)
                    {
                        <ul class="nav" id="side-menu">
                            <li>
                                <a href="/Admin/Index" class="active"><span class="glyphicon glyphicon-dashboard" style="margin-right:10px"></span>Dashboard</a>
                            </li>
                            <li>
                                <a href="/Admin/Adminler"><span class="glyphicon glyphicon-user" style="margin-right:10px"></span>Kullanıcılar</a>
                            </li>
                            <li>
                                <a href="/Services/Index"><span class="glyphicon glyphicon-globe" style="margin-right:10px"></span>Faaliyetler</a>
                            </li>
                            <li>
                                <a href="/ProjectsDone/Index"><span class="glyphicon glyphicon-th-list" style="margin-right:10px"></span>Yapılan Projeler</a>
                            </li>
                            <li>
                                <a href="/Slider/Index"><span class="glyphicon glyphicon-modal-window" style="margin-right:10px"></span>Slider</a>
                            </li>
                            <li>
                                <a href="/About/Index"><span class="glyphicon glyphicon-bookmark" style="margin-right:10px"></span>Şirket Hakkında</a>
                            </li>
                            <li>
                                <a href="/Contact/Index"><span class="glyphicon glyphicon-phone" style="margin-right:10px"></span>İletişim</a>
                            </li>
                        </ul>
                    }
                    <!-- //sidebar-collapse -->
                </nav>
            </div>
        </div>
        <!--left-fixed -navigation-->
        <!-- header-starts -->
        <div class="sticky-header header-section ">
            <div class="header-left">
                <!--toggle button start-->
                <button id="showLeftPush"><i class="fa fa-bars"></i></button>
                <!--toggle button end-->
                <!--logo -->
                <div class="logo">
                    <a href="/Admin/Index">
                        <h1>Doğu Zemin Laboratuvar</h1>
                        <span>Yönetim Paneli</span>
                    </a>
                </div>
                <!--//logo-->
                <div class="clearfix"> </div>
            </div>
            <div class="header-right">
                <div class="profile_details_left">
                    <div class="profile_details">
                        @if (CurrentSession.User != null)
                        {
                            <ul>
                                <li class="dropdown profile_details_drop">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                        <div class="profile_img">
                                            <span class="prfil-img">
                                                <img src="~/images/user/@CurrentSession.User.ProfilImage" width="48" height="48" />
                                            </span>
                                            <div class="user-name">
                                                <p>@CurrentSession.User.Username</p>
                                                @if (CurrentSession.User.IsAdmin)
                                                {
                                                    <span>Administrator</span>
                                                }
                                                else
                                                {
                                                    <span>Standart Kullanıcı</span>
                                                }
                                            </div>
                                            <i class="fa fa-angle-down lnr"></i>
                                            <i class="fa fa-angle-up lnr"></i>
                                            <div class="clearfix"></div>
                                        </div>
                                    </a>
                                    <ul class="dropdown-menu drp-mnu">
                                        <li> <a href="/Admin/LogOut"><i class="fa fa-sign-out"></i> Çıkış</a> </li>
                                    </ul>
                                </li>
                            </ul>
                        }
                    </div>
                    <div class="clearfix"> </div>
                </div>
                <div class="clearfix"> </div>
            </div>
        </div>        <!-- //header-ends -->

        @RenderBody()

        <!--footer-->
        <div class="footer">
            <p>&copy; 2020 Doğu Zemin Laboratuvarı Admin Paneli. Bütün Hakları Saklıdır | <a href="http://wbsoftware.somee.com/" target="_blank">WBSoftware</a> Tarafından Hazırlandı.</p>
        </div>
        <!--//footer-->
    </div>
    <!-- Classie -->
    <script src="~/Content/AdminTemplate/js/classie.js"></script>
    <script>
        var menuLeft = document.getElementById('cbp-spmenu-s1'),
            showLeftPush = document.getElementById('showLeftPush'),
            body = document.body;

        showLeftPush.onclick = function () {
            classie.toggle(this, 'active');
            classie.toggle(body, 'cbp-spmenu-push-toright');
            classie.toggle(menuLeft, 'cbp-spmenu-open');
            disableOther('showLeftPush');
        };


        function disableOther(button) {
            if (button !== 'showLeftPush') {
                classie.toggle(showLeftPush, 'disabled');
            }
        }
    </script>

    @*@Scripts.Render("~/bundles/js")*@
        <!--scrolling js-->
    <script src="~/Content/AdminTemplate/js/jquery.nicescroll.js"></script>
    <script src="~/Content/AdminTemplate/js/scripts.js"></script>
    <!--//scrolling js-->
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Content/AdminTemplate/js/bootstrap.js"></script>

</body>
</html>